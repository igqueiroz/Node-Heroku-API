{
  "name": "react-native-backend",
  "version": "0.0.1",
  "description": "Backend NodeJs API",
  "main": "index.js",
  "repository": "git@bitbucket.org:igqueiroz/mobile-app-native-node.git",
  "author": "Igor Queiroz <igor@igorqueiroz.com.br>",
  "license": "MIT",
  "watch": {
    "extensions": "js,jsx"
  },
  "keywords": [
    "nodejs",
    "heroku",
    "react-native-backend"
  ],
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "node --optimize_for_size --max_old_space_size=960 index.js",
    "deploy:local": "heroku local web",
    "deploy:heroku": "git push heroku main",
    "watch:deploy:local": "npm-watch deploy:local"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "homepage": "https://bitbucket.org/igqueiroz/mobile-app-native-node/src/main/README.md",
  "dependencies": {
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-jsdoc-swagger": "^1.6.6",
    "fast-xml-parser": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pino": "^7.0.5",
    "pino-caller": "^3.2.0",
    "sequelize": "^6.9.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "jest": "^27.3.1",
    "npm-watch": "^0.10.0",
    "pino-pretty": "^7.1.0",
    "supertest": "^6.1.6"
  }
}